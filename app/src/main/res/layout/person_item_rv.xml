<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/blue">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/profile_image_view"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_margin="10dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:paddingBottom="10dp"
        android:src="@drawable/baseline_person_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="124dp"
        android:fontFamily="@font/nt_somic_semibold"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/profile_image_view"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.028"
        tools:text="Username" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageLifeStyle"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:background="@color/blue"
        android:bottomLeftRadius="20dp"
        android:bottomRightRadius="20dp"
        android:fontFamily="@font/nt_somic_bold"
        android:src="@drawable/baseline_work_24"
        android:text="lifestyle"
        android:textColor="@color/black"
        android:textSize="20.5dp"
        android:topLeftRadius="20dp"
        android:topRightRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.427" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imagelandscape"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:layout_marginTop="236dp"
        android:background="@color/blue"
        android:bottomLeftRadius="20dp"
        android:bottomRightRadius="20dp"
        android:fontFamily="@font/nt_somic_bold"
        android:src="@drawable/baseline_landscape_24"
        android:text="landscape"
        android:textColor="@color/black"
        android:textSize="20.5dp"
        android:topLeftRadius="20dp"
        android:topRightRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.411" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imagefood"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:layout_marginStart="138dp"
        android:background="@color/blue"
        android:bottomLeftRadius="20dp"
        android:bottomRightRadius="20dp"
        android:fontFamily="@font/nt_somic_bold"
        android:src="@drawable/baseline_fastfood_24"
        android:text="food"
        android:textColor="@color/black"
        android:textSize="22dp"
        android:topLeftRadius="20dp"
        android:topRightRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.427" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/architecture"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:background="@color/blue"
        android:bottomLeftRadius="20dp"
        android:bottomRightRadius="20dp"
        android:fontFamily="@font/nt_somic_bold"
        android:src="@drawable/baseline_mosque_24"
        android:text="architecture"
        android:textColor="@color/black"
        android:textSize="20.5dp"
        android:topLeftRadius="20dp"
        android:topRightRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/baseline_mosque_24" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/other"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:background="@color/blue"
        android:bottomLeftRadius="20dp"
        android:bottomRightRadius="20dp"
        android:fontFamily="@font/nt_somic_bold"
        android:src="@drawable/baseline_sports_volleyball_24"
        android:text="other"
        android:textColor="@color/black"
        android:textSize="20.5dp"
        android:topLeftRadius="20dp"
        android:topRightRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageLifeStyle"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/baseline_sports_volleyball_24" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/animals"
        android:layout_width="135dp"
        android:layout_height="135dp"
        android:bottomLeftRadius="20dp"
        android:bottomRightRadius="20dp"
        android:background="@color/blue"
        android:fontFamily="@font/nt_somic_bold"
        android:src="@drawable/baseline_pets_24"
        android:text="animals"
        android:textColor="@color/black"
        android:textSize="20.5dp"
        android:topLeftRadius="20dp"
        android:topRightRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageLifeStyle"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/baseline_pets_24" />


</androidx.constraintlayout.widget.ConstraintLayout>